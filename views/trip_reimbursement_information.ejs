<br><br>
<div class="container-fluid tab-content">

<div  id="tri" class=" container tab-pane active">

    <h3>TRIP INFORMATION</h3>
    Get ready for your upcoming trip!
   <br><br>
            <% if(files && backend_doc){ %>
              <% files.forEach(function(file) { %>
                <%backend_doc.forEach(function(uc){%>
                  <%if(uc.filename===file.filename && uc.document_type==='trip' ){%>
                <div class="card card-body mb-3 bg-light">
                  <% if(file.isImage) { %>
                    <img src="/backend/image/<%= file.filename %>" alt="">
                    <% } else { %>
                      <a href="/backend/files/<%= file.filename %>"><%=uc.originalfilename%></a>
                      
                        <% } %>
                         
                </div>
                <%} })}) %>
                  <% } else { %>
                    <p>No files to show</p>
                    <% } %>
        
    </div>
<div  id="reimburse" class=" container tab-pane fade">
    <h3>REIMBURSEMENT INFORMATION</h3>
    This is the documentation sent to the HSE on your behalf
        <br>
        <br>
                <% if(files && backend_doc){ %>
                  <% files.forEach(function(file) { %>
                    <%backend_doc.forEach(function(uc){%>
                      <%if(uc.filename===file.filename && uc.document_type==='reimbursement' ){%>
                    <div class="card card-body mb-3 bg-light">
                      <% if(file.isImage) { %>
                        <img src="/backend/image/<%= file.filename %>" alt="">
                        <% } else { %>
                          <a href="/backend/files/<%= file.filename %>"><%=uc.originalfilename%></a>
                          
                            <% } %>
                             
                    </div>
                    <%} })}) %>
                      <% } else { %>
                        <p>No files to show</p>
                        <% } %>
            
</div>

            </div>
