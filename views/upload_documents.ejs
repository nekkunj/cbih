            <h2 class="  my-4">Waiting list letter</h2>
       <form action="/documents/waiting_list_letter" method="post" enctype="multipart/form-data" > 
        <div id="form1_div"></div>
       
        <div class="custom-file mb-3">
      
    <input type="file" name="file" id="file" class="custom-file-input waiting_list_letter_id"  accept=".doc,.docx,.pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document">
    <!-- accept="image/x-png,image/gif,image/jpeg"> -->
    <div id="waiting_list_letter_warning" style="color:red"></div>  
    <label for="file" class="custom-file-label" data-browse="Browse" >Choose file</label>
    
    </div>
    <input type="submit" value="Upload" id="waiting_list_letter_upload" class="btn btn-success btn-block">
    </form>
    
  </span>
  <span class="col-md-1"></span>

        <span class="col-md-3 col-sm-6" >
    <% if(files && user_doc){ %>
        <% files.forEach(function(file) { %>
          <%user_doc.forEach(function(uc){%>
            <%if(uc.filename===file.filename && uc.type_of_file==='waiting_list_letter'){%>
          <div class="card card-body " >
            <% if(file.isImage) { %>
              <img src="image/<%= file.filename %>" alt="">
              <% } else { %>
                <a href="files/<%= file.filename %>"><%=uc.originalfilename%></a>
                  <% } %>
                    <form method="POST" action="/files/<%= file._id %>?_method=DELETE" >
                      <button class="btn btn-danger btn-block mt-4"> Delete</button>
                    </form>
          </div>
          <%} })}) %>
            <% } else { %>
              <p>No files to show</p>
              <% } %>
        </span>
    </div>
    <hr>
